<%= form_for @user, url: login_path do |f| %>

 <%= render 'shared/error_messages' %>
 

 <div class="field">
  <%= f.label "Username:" %>
  <%= f.text_field :username %><br><br>
</div>

<div class="field">
  <%= f.label "Password:" %>
  <%= f.password_field :password %><br><br>
</div>
  <%= f.submit "Log In" %>

<% end %>
<br>